package com.cioclass.domain.guns;

import javax.persistence.Column;

public class GunsDept {
	@Column(name="id",length=11,columnDefinition="")
	private Integer id;
	@Column(name="num",length=11,columnDefinition="")
	private Integer num;
	@Column(name="pid",length=11,columnDefinition="")
	private Integer pid;
	@Column(name="pids",length=255,columnDefinition="")
	private String pids;
	@Column(name="simplename",length=45)
	private String simpleName;
	@Column(name="fullname",length=255)
	private String fullName;
	@Column(name="tips",length=255)
	private String tips;
	@Column(name="version",length=11,columnDefinition="")
	private Integer version;

}
