		String url = "http://cioclass.com/o";
		int size = 8;
		int num = 100000;
		qb.createContent(en, bn, url, size, num);
	}
	
	@Test
	public void testFindByEnBnNum() {
		List<>
		String en = "43";
		String bn = "13";
		String numStr = "000 123";
		qb.findByEnBnNum(en, bn, numStr);
	}
}
