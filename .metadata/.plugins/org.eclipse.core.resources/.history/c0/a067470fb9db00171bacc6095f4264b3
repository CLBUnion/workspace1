package com.cioclass.domain.or;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
/**
 * 追溯类别
 */
@Entity
public class ORTraceType implements Serializable{
	private static final long serialVersionUID = 2214845363946778609L;
	/**
	 * 主键，唯一
	 */
	private Integer id;
	/**
	 * 追溯类别名称
	 */
	private String typeName;
	
	
	public ORTraceType() {
	}
	/**
     * Getter for property 'id'.
     *
     * @return Value for property 'id'.
     */
    @GeneratedValue
	@Id
	public Integer getId() {
		return id;
	}
	/**
     * Setter for property 'id'.
     *
     * @param id Value to set for property 'id'.
     */
    public void setId(Integer id) {
		this.id = id;
	}
	/**
     * Getter for property 'typeName'.
     *
     * @return Value for property 'typeName'.
     */
    public String getTypeName() {
		return typeName;
	}
	/**
     * Setter for property 'typeName'.
     *
     * @param typeName Value to set for property 'typeName'.
     */
    public void setTypeName(String typeName) {
		this.typeName = typeName;
	}
    @Override
	public String toString() {
		return "ORTraceType [id=" + id + ", typeName=" + typeName + "]";
	}
    @Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((typeName == null) ? 0 : typeName.hashCode());
		return result;
	}
    @Override
	public boolean equals(Object obj) {
		if (this == obj) {
            return true;
        }
		if (obj == null) {
            return false;
        }
		if (getClass() != obj.getClass()) {
            return false;
        }
		ORTraceType other = (ORTraceType) obj;
		if (typeName == null) {
			if (other.typeName != null) {
                return false;
            }
		} else if (!typeName.equals(other.typeName)) {
            return false;
        }
		return true;
	}
	
}
