package com.cioclass.domain.guns;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.Index;
import javax.persistence.Table;
@Entity
@Table(name = "guns_dept",indexes=@Index(columnList="id",unique=true))
public class GunsDept {
	private Integer id;
	private Integer num;
	private Integer pid;
	private String pids;
	private String simpleName;
	private String fullName;
	private String tips;
	private Integer version;
	public GunsDept() {
		
	}
	@Id
	@GeneratedValue
	@Column(name = "id", length = 11, columnDefinition = "int(11) COMMENT '主键id'")
	public Integer getId() {
		return id;
	}

	@Column(name = "num", length = 11, columnDefinition = "序号")
	public Integer getNum() {
		return num;
	}

	@Column(name = "pid", length = 11, columnDefinition = "父部门id")
	public Integer getPid() {
		return pid;
	}

	@Column(name = "pids", length = 255, columnDefinition = "父级ids")
	public String getPids() {
		return pids;
	}

	@Column(name = "simplename", length = 45, columnDefinition = "简称")
	public String getSimpleName() {
		return simpleName;
	}

	@Column(name = "fullname", length = 255, columnDefinition = "全称")
	public String getFullName() {
		return fullName;
	}

	@Column(name = "tips", length = 255, columnDefinition = "提示")
	public String getTips() {
		return tips;
	}

	@Column(name = "version", length = 11, columnDefinition = "版本（乐观锁保留字段）")
	public Integer getVersion() {
		return version;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public void setNum(Integer num) {
		this.num = num;
	}

	public void setPid(Integer pid) {
		this.pid = pid;
	}

	public void setPids(String pids) {
		this.pids = pids;
	}

	public void setSimpleName(String simpleName) {
		this.simpleName = simpleName;
	}

	public void setFullName(String fullName) {
		this.fullName = fullName;
	}

	public void setTips(String tips) {
		this.tips = tips;
	}

	public void setVersion(Integer version) {
		this.version = version;
	}

	@Override
	public String toString() {
		return "GunsDept [id=" + id + ", num=" + num + ", pid=" + pid + ", pids=" + pids + ", simpleName=" + simpleName
				+ ", fullName=" + fullName + ", tips=" + tips + ", version=" + version + "]";
	}

}
