package com.cioclass.domain.or;

import java.io.Serializable;
import java.util.List;

import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToMany;
import javax.persistence.OneToOne;

/**
 * 追溯信息
 */
@Entity
public class ORRetroactiveInfo implements Serializable {
	private static final long serialVersionUID = 4032680221448536155L;
	/**
	 * 主键，唯一
	 */
	private Integer id;
	/**
	 * 主要内容
	 */
	private String content;
	/**
	 * 全局变量(OrTraceType)
	 */
	private Integer traceType;
	/**
	 * 批次信息
	 */
	private List<ORBatchInfo> oRBatchInfo;
	
	
	/** Constructs a new ORRetroactiveInfo. */
    public ORRetroactiveInfo() {
	}

	public ORRetroactiveInfo(Integer id, String content, Integer traceType) {
		this.id = id;
		this.content = content;
		this.traceType = traceType;
	}

	/**
     * Getter for property 'traceType'.
     *
     * @return Value for property 'traceType'.
     */
    @OneToOne
	@JoinColumn
	public Integer getTraceType() {
		return traceType;
	}

	/**
     * Setter for property 'traceType'.
     *
     * @param traceType Value to set for property 'traceType'.
     */
    public void setTraceType(ORTraceType traceType) {
		this.traceType = traceType;
	}

	/**
     * Getter for property 'id'.
     *
     * @return Value for property 'id'.
     */
    @GeneratedValue
	@Id
	public Integer getId() {
		return id;
	}

	/**
     * Setter for property 'id'.
     *
     * @param id Value to set for property 'id'.
     */
    public void setId(Integer id) {
		this.id = id;
	}

	/**
     * Getter for property 'content'.
     *
     * @return Value for property 'content'.
     */
    public String getContent() {
		return content;
	}

	/**
     * Setter for property 'content'.
     *
     * @param content Value to set for property 'content'.
     */
    public void setContent(String content) {
		this.content = content;
	}
	/**
     * Getter for property 'oRBatchInfo'.
     *
     * @return Value for property 'oRBatchInfo'.
     */
    @ManyToMany(mappedBy="retroactiveInfoIds",fetch=FetchType.EAGER)
	public List<ORBatchInfo> getoRBatchInfo() {
		return oRBatchInfo;
	}

	/**
     * Setter for property 'oRBatchInfo'.
     *
     * @param oRBatchInfo Value to set for property 'oRBatchInfo'.
     */
    public void setoRBatchInfo(List<ORBatchInfo> oRBatchInfo) {
		this.oRBatchInfo = oRBatchInfo;
	}
	/** {@inheritDoc} */
    @Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((content == null) ? 0 : content.hashCode());
		return result;
	}

	/** {@inheritDoc} */
    @Override
	public boolean equals(Object obj) {
		if (this == obj) {
            return true;
        }
		if (obj == null) {
            return false;
        }
		if (getClass() != obj.getClass()) {
            return false;
        }
		ORRetroactiveInfo other = (ORRetroactiveInfo) obj;
		if (content == null) {
			if (other.content != null) {
                return false;
            }
		} else if (!content.equals(other.content)) {
            return false;
        }
		return true;
	}

	/** {@inheritDoc} */
    @Override
	public String toString() {
		return "ORRetroactiveInformation [id=" + id + ", content=" + content + ", traceType=" + traceType + "]";
	}

	
}
