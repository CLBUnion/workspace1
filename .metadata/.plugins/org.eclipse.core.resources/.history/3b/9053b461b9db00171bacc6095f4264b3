package com.cioclass.service.or.impl;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.cioclass.dao.or.OrTraceTypeDao;
import com.cioclass.domain.or.ORTraceType;
import com.cioclass.service.or.OrTraceTypeService;

/**
 * 追溯类别服务实现类
 */
@Service
public class OrTraceTypeServiceImpl implements OrTraceTypeService {
	@Autowired
	private OrTraceTypeDao orTraceTypeDao;

    /** {@inheritDoc} */
	@Override
	public ORTraceType findById(Integer id) {
		return orTraceTypeDao.findOne(id);
	}

    /** {@inheritDoc} */
	@Override
	public void deleteById(Integer id) {
		orTraceTypeDao.delete(id);
	}

    /** {@inheritDoc} */
	@Override
	public boolean exists(Integer id) {
		return orTraceTypeDao.exists(id);
	}

}
