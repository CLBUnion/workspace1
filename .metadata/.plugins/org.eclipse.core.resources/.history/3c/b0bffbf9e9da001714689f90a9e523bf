package com.cioclass.util.or;

import java.util.List;
import java.util.Set;

public interface IQrcodeBuilder {
	List<String> createContent(String en, String bn, String url, int size, int num);
	String getNumStr(int no,int size);
	/**
	 * 获取字符串集合
	 * 
	 * @param size
	 *            随机号码长度
	 * @param num
	 *            生成内容数量
	 * @return
	 */
	Set<String> getSet(int size, int num);
}
